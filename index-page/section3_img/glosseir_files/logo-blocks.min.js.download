(()=>{class e extends HTMLElement{constructor(){super();const e=document.querySelectorAll(".logo-blocks__container__logos__item");if(e){const o=this;e.forEach((t,e)=>{t.addEventListener("mouseenter",function(e){o.removeAllActiveClass(),o.bindEvents(t)}),t.addEventListener("click",function(e){o.removeAllActiveClass(),o.bindEvents(t)})})}}removeAllActiveClass(){document.querySelectorAll(".logo-blocks__container__logos__item").forEach(e=>e.classList.remove("active")),document.querySelectorAll(".logo-blocks__container__copies__item").forEach(e=>e.classList.remove("active"))}bindEvents(e){const t=document.getElementById(e.dataset.target);e.classList.add("active"),t.classList.add("active")}}customElements.get("section-logo-blocks")||customElements.define("section-logo-blocks",e)})();